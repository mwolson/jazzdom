{
  "name": "jazzdom",
  "version": "0.1.0",
  "description": "Promisified jsdom setup and teardown for your test runner.",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-s build:clean build:js",
    "build:clean": "rimraf lib",
    "build:js": "babel -d lib/ src/",
    "lint": "eslint src --ext .js",
    "prepublish": "run-s lint build",
    "test": "mocha \"src/**/.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mwolson/jazzdom.git"
  },
  "author": "Michael Olson <me@mwolson.org>",
  "license": "MIT",
  "keywords": [
    "jest",
    "mocha",
    "jsdom",
    "dom",
    "test"
  ],
  "peerDependencies": {
    "jsdom": "*",
    "jsdom-global": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "jsdom": "^9.12.0",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.2.0",
    "mocha-clean": "^1.0.0",
    "mocha-lcov-reporter": "^1.0.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1"
  }
}
